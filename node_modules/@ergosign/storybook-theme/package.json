{
  "name": "@ergosign/storybook-theme",
  "version": "1.0.1",
  "description": "Ergosign Theme for Storybook",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch --outDir dist",
    "release-it-beta": "release-it -n --preRelease=beta --npm.tag=next --verbose --ci --no-git.requireCleanWorkingDir",
    "release-it-patch": "release-it patch -n --verbose --ci --no-git.requireCleanWorkingDir",
    "release-it-minor": "release-it minor -n --verbose --ci --no-git.requireCleanWorkingDir",
    "release-it-major": "release-it major -n --verbose --ci --no-git.requireCleanWorkingDir",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.ergosign-projects.com:ergosign/web/ergosign-storybook-theme.git"
  },
  "keywords": [
    "storybook",
    "theme"
  ],
  "author": "philipp.schardt",
  "license": "ISC",
  "devDependencies": {
    "@storybook/theming": "6.5.9",
    "release-it": "12.6.1",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "@storybook/theming": "^6"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "scope": "@ergosign"
  }
}
